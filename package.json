{
  "name": "write-cafe",
  "version": "1.0.0",
  "description": "A web-based English writing assistant with automatic grammar error correction",
  "main": "server/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "node server/index.js",
    "api": "python server/app.py",
    "build": "rimraf public && set NODE_ENV=production && webpack --config ./webpack.prod.config.js --progress --profile --colors",
    "start": "npm run build && NODE_ENV=production PORT=3100 node server/index.js",
    "forever-start": "npm run build && NODE_ENV=production PORT=3100 forever start server/index.js --uid write-cafe",
    "forever-stop": "forever stop server/index.js --uid write-cafe"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HowardLoTW/nlp-term-project.git"
  },
  "keywords": [
    "react",
    "node",
    "python",
    "nlp",
    "English writing",
    "grammar error correction"
  ],
  "dependencies": {
    "classnames": "^2.2.1",
    "cors": "^2.7.1",
    "draft-js": "^0.7.0",
    "express": "^4.13.4",
    "history": "^2.1.1",
    "immutable": "^3.8.1",
    "install": "^0.7.3",
    "lodash": "^4.0.0",
    "moment": "^2.12.0",
    "npm": "^3.9.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.4",
    "redux": "^3.0.5",
    "redux-thunk": "^2.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.3.21",
    "babel-loader": "^6.2.0",
    "babel-polyfill": "^6.8.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "bootstrap-loader": "^1.0.7",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "forever": "^0.15.1",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.15.0",
    "json-loader": "^0.5.4",
    "jsx-loader": "^0.13.2",
    "node-sass": "^3.7.0",
    "normalize.css": "^4.1.1",
    "npm-check-updates": "^2.5.6",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "resolve-url-loader": "^1.4.3",
    "rimraf": "^2.5.2",
    "sass-loader": "^3.1.2",
    "sass-resources-loader": "^1.0.1",
    "stats-webpack-plugin": "^0.3.1",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-dev-server": "^1.14.0",
    "webpack-hot-middleware": "^2.10.0"
  },
